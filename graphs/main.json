{
    "properties": {
        "name": "main",
        "environment": {
            "type": "noflo-nodejs"
        },
        "description": "Backup runner",
        "icon": "download"
    },
    "inports": {},
    "outports": {},
    "groups": [
        {
            "name": "Subgallery",
            "nodes": [],
            "metadata": {
                "description": ""
            }
        },
        {
            "name": "Photo",
            "nodes": [],
            "metadata": {
                "description": ""
            }
        }
    ],
    "processes": {
        "core/Repeat_d74dk": {
            "component": "core/Repeat",
            "metadata": {
                "label": "core/Repeat",
                "x": 36,
                "y": 180,
                "width": 72,
                "height": 72
            }
        },
        "FindPhotoPages": {
            "component": "routabackup/FindPhotos",
            "metadata": {
                "label": "FindPhotoPages",
                "x": 180,
                "y": 180,
                "width": 72,
                "height": 72
            }
        },
        "routabackup/GetPhotos": {
            "component": "routabackup/GetPhotos",
            "metadata": {
                "label": "GetPhotos",
                "x": 324,
                "y": 180,
                "width": 72,
                "height": 72
            }
        },
        "Collect": {
            "component": "adapters/PacketsToArray",
            "metadata": {
                "label": "Collect",
                "x": 432,
                "y": 180,
                "width": 72,
                "height": 72
            }
        },
        "ToJson": {
            "component": "strings/Jsonify",
            "metadata": {
                "label": "ToJson",
                "x": 576,
                "y": 180,
                "width": 72,
                "height": 72
            }
        },
        "SaveJson": {
            "component": "filesystem/WriteFile",
            "metadata": {
                "label": "SaveJson",
                "x": 720,
                "y": 180,
                "width": 72,
                "height": 72
            }
        }
    },
    "connections": [
        {
            "src": {
                "process": "core/Repeat_d74dk",
                "port": "out"
            },
            "tgt": {
                "process": "FindPhotoPages",
                "port": "in"
            },
            "metadata": {
                "route": null
            }
        },
        {
            "src": {
                "process": "FindPhotoPages",
                "port": "out"
            },
            "tgt": {
                "process": "routabackup/GetPhotos",
                "port": "in"
            },
            "metadata": {
                "route": null
            }
        },
        {
            "src": {
                "process": "routabackup/GetPhotos",
                "port": "out"
            },
            "tgt": {
                "process": "Collect",
                "port": "in"
            },
            "metadata": {}
        },
        {
            "src": {
                "process": "Collect",
                "port": "out"
            },
            "tgt": {
                "process": "ToJson",
                "port": "in"
            },
            "metadata": {}
        },
        {
            "src": {
                "process": "ToJson",
                "port": "out"
            },
            "tgt": {
                "process": "SaveJson",
                "port": "in"
            },
            "metadata": {}
        },
        {
            "data": "http://www.routamc.org/gallery/black-sea-2004/",
            "tgt": {
                "process": "core/Repeat_d74dk",
                "port": "in"
            }
        },
        {
            "data": true,
            "tgt": {
                "process": "ToJson",
                "port": "pretty"
            }
        },
        {
            "data": "photos.json",
            "tgt": {
                "process": "SaveJson",
                "port": "filename"
            }
        }
    ]
}